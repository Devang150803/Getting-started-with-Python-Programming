def maximum(a,b,c):
    
    if(a>b):
        if(a>c):
            return a
        else: return c
        
    else: 
        if(b>c):
            return b
        else:
            return c
        
m=maximum(input("1st :-"), input("2nd :-"), input("3rd :- "))
        
print("The greatest value among this is :- " + str(m))
